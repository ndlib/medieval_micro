<%= semantic_form_for [:admin, @date_range]  do |form| %>
  <%= form.inputs do %>
    <%= form.input :code %>
    <%= form.input :start_date, start_year: 100, max_years_allowed: Time.now.year %>
    <%= form.input :end_date, start_year: 100, max_years_allowed: Time.now.year %>
  <% end %>
  <%= form.action :submit, :as => :button %>
<% end %>
